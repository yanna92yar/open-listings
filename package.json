    "ajv-i18n": "^4.2.0",
    "async-mutex": "^0.4.0",
    "ava": "^5.2.0",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "bloom-filters": "^3.0.0",
    "colorette": "^2.0.20",
    "config": "^3.3.9",
    "dotenv": "^16.0.3",
    "email-to-name": "^2.0.4",
    "email-validator": "^2.0.4",
