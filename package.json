{
  "name": "open-listings",
  "version": "1.0.0",
  "description": "A performant listings website with maps and tags",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "lint": "eslint libs/**/*.js",
    "lint-fix": "eslint --fix libs/**/*.js",
    "pretty": "prettier --ignore-unknown --write libs/**/*.js",
    "prepare": "husky install",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint",
    "build": "sudo apt-get install -y build-essential golang-go && npm install && cd client && npm install && npm run build && curl -sf https://gobinaries.com/tj/node-prune | sh",
    "download:assets": "curl https://raw.githubusercontent.com/stamparm/ipsum/master/ipsum.txt > data/raw/ipsum.txt",
    "start": "node server",
    "start:dev": "cross-env DEBUG=app:* nodemon server",
    "stop": "pkill --signal SIGINT open-listings",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:e2e": "NODE_ENV=api tap --no-coverage && ava",
    "doc": "rimraf docs && jsdoc -c jsdoc.conf.json",
    "postinstall": "node-prune"
  },
  "type": "module",
  "keywords": [
    "Node.js",
    "JavaScript",
    "MongoDB",
    "Maps",
    "web-app"
  ],
  "license": "",
  "engines": {
    "node": ">=17.9.1"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/auth": "^4.2.0",
    "@fastify/compress": "^6.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/flash": "^5.1.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/jwt": "^6.7.1",
    "@fastify/mongodb": "^6.2.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/redis": "^6.1.0",
    "@fastify/schedule": "^4.1.0",
    "@fastify/session": "^10.1.1",
    "@fastify/static": "^6.9.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.4.0",
    "@fastify/view": "^7.4.1",
    "@fastify/websocket": "^7.2.0",
    "@google-cloud/storage": "^6.9.3",
    "@gquittet/graceful-server": "^4.0.4",
    "@seald-io/nedb": "^4.0.2",
    "@toreda/adt": "^0.4.0",
